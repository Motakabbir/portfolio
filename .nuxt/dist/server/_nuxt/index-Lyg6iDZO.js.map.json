{"file":"index-Lyg6iDZO.js","mappings":";;;;;;;AAgFA,UAAM,OAAO,IAAI,aAAa;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;","names":[],"sources":["../../../../pages/index.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Hero Section -->\n    <section class=\"bg-gradient-to-r from-primary to-blue-700 text-white py-20\">\n      <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div class=\"text-center\">\n          <h1 class=\"text-4xl font-bold sm:text-5xl md:text-6xl\">\n            {{ data.personal.name }}\n          </h1>\n          <p class=\"mt-3 text-xl\">{{ data.personal.title }}</p>\n          <p class=\"mt-5 max-w-xl mx-auto text-xl\">{{ data.personal.bio }}</p>\n        </div>\n      </div>\n    </section>\n\n    <!-- Skills Section -->\n    <section class=\"py-12 bg-white\">\n      <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <h2 class=\"text-3xl font-bold text-center mb-8\">Skills & Expertise</h2>\n        <div class=\"grid grid-cols-1 md:grid-cols-2 gap-8\">\n          <div v-for=\"skillSet in data.skills\" :key=\"skillSet.category\" \n               class=\"bg-gray-50 p-6 rounded-lg shadow-sm\">\n            <h3 class=\"text-xl font-semibold mb-4\">{{ skillSet.category }}</h3>\n            <div class=\"flex flex-wrap gap-2\">\n              <span v-for=\"skill in skillSet.items\" :key=\"skill\"\n                    class=\"bg-primary text-white px-3 py-1 rounded-full text-sm\">\n                {{ skill }}\n              </span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- Featured Projects -->\n    <section class=\"py-12 bg-gray-50\">\n      <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <h2 class=\"text-3xl font-bold text-center mb-8\">Featured Projects</h2>\n        <div class=\"grid grid-cols-1 md:grid-cols-2 gap-8\">\n          <div v-for=\"project in data.projects\" :key=\"project.id\"\n               class=\"bg-white rounded-lg shadow-sm overflow-hidden\">\n            <img :src=\"project.image\" :alt=\"project.title\" class=\"w-full h-48 object-cover\">\n            <div class=\"p-6\">\n              <h3 class=\"text-xl font-semibold mb-2\">{{ project.title }}</h3>\n              <p class=\"text-gray-600 mb-4\">{{ project.description }}</p>\n              <div class=\"flex flex-wrap gap-2 mb-4\">\n                <span v-for=\"tech in project.technologies\" :key=\"tech\"\n                      class=\"bg-gray-100 text-gray-800 px-2 py-1 rounded text-sm\">\n                  {{ tech }}\n                </span>\n              </div>\n              <a :href=\"project.link\" class=\"text-primary hover:underline\">View Project â†’</a>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n\n    <!-- Latest Blog Posts -->\n    <section class=\"py-12 bg-white\">\n      <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <h2 class=\"text-3xl font-bold text-center mb-8\">Latest Blog Posts</h2>\n        <div class=\"grid grid-cols-1 md:grid-cols-2 gap-8\">\n          <div v-for=\"post in data.blogPosts\" :key=\"post.id\"\n               class=\"bg-gray-50 rounded-lg shadow-sm overflow-hidden\">\n            <img :src=\"post.image\" :alt=\"post.title\" class=\"w-full h-48 object-cover\">\n            <div class=\"p-6\">\n              <h3 class=\"text-xl font-semibold mb-2\">{{ post.title }}</h3>\n              <p class=\"text-gray-600 mb-2\">{{ post.excerpt }}</p>\n              <p class=\"text-sm text-gray-500\">{{ new Date(post.date).toLocaleDateString() }}</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script setup>\nimport portfolioData from '~/data/portfolio.json'\nconst data = ref(portfolioData)\n</script>\n"],"version":3}